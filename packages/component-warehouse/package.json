{
  "name": "@apac-ui-warehouse/component-warehouse",
  "version": "1.4.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/styles.css": "./dist/styles.css",
    "./dist/assets/*": "./dist/assets/*",
    "./theme/colors": "./src/theme/colors.ts",
    "./theme/colors.scss": "./src/theme/colors.scss",
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "**/*.less",
    "./src/styles.ts"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "dev": "vite build --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,less,scss,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,less,scss,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "package": "npm run build-storybook && cp -v -r storybook-static/ ../../../apac-realtime-process-manager/apac_realtime_process_manager/rest/ui/storybook/"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "antd": "^5.0.0",
    "framer-motion": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^23.0.1",
    "oxlint": "^0.15.0",
    "prettier": "^3.2.5",
    "react-router-dom": "^7.9.6",
    "sass": "^1.94.1",
    "storybook": "^7.6.17",
    "typescript": "^5.3.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@ant-design/icons": "^5.0.0",
    "antd": "^5.0.0",
    "framer-motion": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.0 || ^7.0.0"
  }
}
